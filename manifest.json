{
  "manifest_version": 2,
  "name": "Search Result Keybindings",
  "version": "1.0",

  "description": "Creates keybindings for Google and other search result pages",

  "icons": {
    "48": "icons/icon-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.google.com/search*"],
      "js": ["search-keybindings.js"],
      "css": ["search-keybindings.css"]
    }
  ],


  "options_ui": {
    "page": "options.html"
  },

  "permissions": ["storage"]
}
